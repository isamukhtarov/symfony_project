<div class="container">
    <div class="row">
        <div class="col-12">
            <div class="filter-links stories-links">
                <ul>
                    {% for story in stories %}

                        <li>
                            <a href="{{ path('story_view', {slug: story.slug}) }}">{{ story.title }}</a>
                        </li>

                    {% endfor %}
                </ul>
            </div>
        </div>
    </div>
</div>
